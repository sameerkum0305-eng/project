<html lang="en">
<head>
<style>

</style>
</head>

<body>
<h1>Current Users</h1>

<table>
    <tr>
        <td>First Name</td>
        <td>Last Name</td>
        <td>Gender</td>
        <td>Age</td>
    </tr>

    <% users.forEach(user=>{ %>
    <tr>
        <td>
            <%= user.firstName %>
        </td>
        <td>
            <%= user.lastName %>
        </td>
        <td>
            <%= user.gender %>
        </td>
        <td>
            <%= user.age %>
        </td>
    </tr>
    <% }); %>

</table>

</body>
</html>
